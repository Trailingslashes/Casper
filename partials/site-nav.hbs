<nav class="site-nav">
    <div class="site-nav-left">
        {{^is "home"}}
            {{#if @blog.logo}}
                <a class="site-nav-logo" href="{{@blog.url}}"><img src="{{@blog.logo}}" alt="{{@blog.title}}" /></a>
            {{else}}
                <a class="site-nav-logo" href="{{@blog.url}}">{{@blog.title}}</a>
            {{/if}}
        {{/is}}
        {{#if @blog.navigation}}
            {{navigation}}
        {{/if}}
    </div>
    <div class="site-nav-right">
        <div class="social-links"></div>
        {{#if @labs.subscribers}}
            <a class="subscribe-button" href="#subscribe">Subscribe</a>
        {{else}}
            <a class="social-link social-link-li" href="https://feedly.com/i/subscription/feed/{{@blog.url}}/rss/" target="_blank" rel="noopener">{{> "icons/rss"}}</a>
        {{/if}}
	 <a class="social-link social-link-li" href="https://www.linkedin.com/in/cole-phillips-9b0b7073/" target="_blank" rel="noopener">{{> "icons/linkedin" }}</a>
         <a class="social-link social-link-li" href="https://github.com/Trailingslashes/" target="_blank" rel="noopener">{{> "icons/github" }}</a>
    </div>
</nav>
